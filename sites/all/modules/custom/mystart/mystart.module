<?php/** * @file * Build page and form for mystart. *//** * Implements hook_menu(). */function mystart_menu() {  $items = array();  $items['mystart-page'] = array(    'title' => 'Mystart',    'page callback' => 'drupal_get_form',    'page arguments' => array('mystart_page_form'),    'access callback' => TRUE,  );  return $items;}/** * Build mystart form. */function mystart_page_form($form, $form_state) {  $form = array();  $form['number1'] = array(    '#type' => 'textfield',    '#title' => t('Number 1'),    '#size' => 20,  );  $form['number2'] = array(    '#type' => 'textfield',    '#title' => t('Number 2'),    '#size' => 20,  );  $form['operator'] = array(    '#type' => 'select',    '#title' => t('Operator'),    '#options' => array('+', '-'),  );  $form['submit'] = array(    '#type' => 'submit',    '#value' => t('Mystart'),  );  return $form;}